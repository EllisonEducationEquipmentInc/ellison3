%h3 My Account
#myaccount_tabs.rounded
  %ul.tabs
    %li.rounded_top= link_to "Profile", "#profile"
    - @tabs.each do |tab|
      %li.rounded_top= link_to "<span>&nbsp;</span>#{tab[1]}".html_safe, :action => tab[0]
  .clear
  #profile= render :partial => 'profile'
:javascript
  $(function() {
  	$("#myaccount_tabs").tabs({cache: true, spinner: '<img src="/images/ui-objects/ajax-loader.gif" />', ajaxOptions: { cache: false } });
  });

