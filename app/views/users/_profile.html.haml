.clear
- if @profile
  %p.dontprint.notice= @profile
%strong Login/Email:
= get_user.email
= link_to "change", edit_user_registration_path(get_user), :remote => true
%br
%strong Password:
xxxxxxxxxxxxxx
= link_to "change", edit_user_registration_path(get_user), :remote => true
%br
%strong Name/Contact Person:
= get_user.name
= link_to "change", edit_user_registration_path(get_user), :remote => true
- if is_ee? || is_er?
  %br
  %strong Company/School/Organization:
  = get_user.company
  = link_to "change", edit_user_registration_path(get_user), :remote => true
- if is_ee_us?
  %br
  %strong Institution Description:
  = institutions.detect {|e| e[1] == "#{get_user.institution}"}[0] rescue "<em>not specified</em>".html_safe
  = link_to "change", edit_user_registration_path(get_user), :remote => true