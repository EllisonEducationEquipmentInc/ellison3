- if @cch && @cch.success? 
  == $('#tax_transaction').html('#{@order.tax_transaction}')
  == $('#tax_calculated_at').html('#{l @order.tax_calculated_at, :format => :custom}')
- elsif @cch
  == alert('#{@cch.errors}');
$('#calculate_tax_button').button({'disabled': false});
$('#tax_amount').removeClass('error');